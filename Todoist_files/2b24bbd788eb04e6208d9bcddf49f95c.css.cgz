.AmiMenu .lbl {
    text-align: left !important;
    color: #aaa;
    font-size: 11px
}
.AmiMenu .menu_buttons {
    margin: 0;
    padding: 0
}
.AmiMenu .menu_buttons li {
    margin: 0;
    padding: 0;
