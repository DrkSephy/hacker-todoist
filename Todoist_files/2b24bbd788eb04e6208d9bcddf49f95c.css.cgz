.dp_container {
    position: absolute;
    z-index: 2000;
    max-width: 275px;
    padding: 4px;
    background-color: #fff;
    border-radius: 6px;
    box-shadow: 0 2px 10px rgba(0, 0, 0, .3)
}
.dp_rerendered {
    animation-name: none !important;
    animation-duration: 0ms
}
.dp_container .month_year {
    font-weight: bold;
    font-size: 13px;
    color: #5c5c5c
}
.dp_cal,
.dp_cal table {
    min-width: 210px
}
.dp_cal {
    background-color: #fff;
    margin: 0 0 3px 0
}
.dp_cal table {
    border-collapse: collapse;
    border-spacing: 0;
    margin: auto;
    margin-bottom: 10px
}
.dp_cal select option {
    padding: 1px 3px
}
.dp_cal th img {
    border: 0;
    vertical-align: middle
}
.dp_cal a span {
    padding: 4px 2px
}
.dp_cal a:hover {
    background-color: #db4c3f;
    color: white !important
}
a.dp_nav:hover {
    background-color: transparent !important;
    opacity: 1
}
.dp_cal th,
.dp_cal td {
    white-space: nowrap;
    width: 14.2857%;
    text-align: center;
    font-size: 11px;
    padding: 5px 0
}
.dp_cal th {
    color: #b3b3b3;
    font-weight: bold;
    padding: 2px 3px;
    font-size: 12px;
    text-transform: uppercase
}
.dp_cal td {
    cursor: pointer;
    border-radius: 15px;
    color: #5c5c5c
}
.dp_cal td.dp_roll {
    color: #fff;
    background: #db4c3f
}
.dp_today {
    font-weight: bold;
    background-color: #eee
}
.dp_selected {
    color: #fff !important;
    background: #111 !important
}
.dp_disabled {
    background: #eee;
    color: #999;
    cursor: default !important
}
.dp_no_due a {
    display: block;
    color: black;
    text-align: center;
    font-size: 12px;
    text-decoration: none !important;
    padding: 8px 3px;
    margin: 0 11px;
    border-top: 1px solid #eee
}
.dp_no_due img {
    vertical-align: middle;
    margin-right: 3px
}
.dp_nav_top {
    padding: 5px 3px 8px 3px !important
}
.dp_nav {
    padding: 4px;
    opacity: .47;
    -webkit-transition: opacity .2s ease-in;
    transition: opacity .2s ease-in
}
.dp_old_day {
    color: #c4c4c4 !important
}
.due_date_disabled {
    color: #555
}
.dp_container .arrow_top {
    position: absolute !important;
    margin-top: -10px !important
}
.dp_arr_left_pos {
    text-align: left !important
}
.uploader_dropzone {
    font-size: 12px;
    padding: 20px 10px;
    border: 3px dashed #ddd;
    border-radius: 3px;
    margin-bottom: 10px
}
.uploader_dropzone img {
    vertical-align: middle;
    margin-right: 5px;
    cursor: pointer
}
.uploader_dropzone ul {
    margin: 0;
    list-style: none;
    padding: 0
}
.uploader_dropzone li {
    border-bottom: 1px solid #ddd;
    padding: 5px 0
}
.uploader_dropzone b {
    margin-left: 5px;
    color: #db4c3f
}
.uploader_dropzone .file_input_container {
    cursor: pointer;
    display: inline-block;
    overflow: hidden;
    position: relative;
    vertical-align: bottom
}
.uploader_dropzone input {
    cursor: pointer !important;
    position: absolute;
    display: inline-block;
    font-size: 30px;
    height: 100%;
    left: 0;
    opacity: .01;
    top: 0;
    width: 100%
}
